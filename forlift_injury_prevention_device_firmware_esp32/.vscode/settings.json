{
    "files.associations": {
        "fstream": "cpp",
        "sstream": "cpp",
        "array": "cpp",
        "deque": "cpp",
        "string": "cpp",
        "unordered_map": "cpp",
        "unordered_set": "cpp",
        "vector": "cpp",
        "string_view": "cpp",
        "initializer_list": "cpp"
    },
    "editor.formatOnSave": true,
    "[python]": {
        "editor.defaultFormatter": "ms-python.autopep8"
    },
    "autopep8.importStrategy": "useBundled",
    "autopep8.args": [
        "--max-line-length",
        "120"
    ]
}